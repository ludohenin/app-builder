{
  "name": "app-buildr",
  "version": "0.0.0",
  "description": "build tasks framework",
  "main": "app-buildr.ts",
  "scripts": {
    "clean": "rimraf example/**/*.js spec/**/*.js src/**/*.js app-buildr.js",
    "compile": "tsc",
    "example": "ts-node example/buildfile.ts",
    "lint": "tslint **/*.ts",
    "postinstall": "tsd reinstall --clean && tsd link",
    "reinstall": "rimraf node_modules && npm cache clean && npm install",
    "test": "npm run clean && npm run lint && npm run compile && jasmine",
    "test.dev": "watch jasmine --wait=0.5"
  },
  "keywords": [
    "build",
    "tasks"
  ],
  "author": "Ludovic Henin <ludovic.henin@yahoo.com>",
  "license": "MIT",
  "dependencies": {
    "angular2": "2.0.0-alpha.46",
    "extend": "^3.0.0",
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "del": "^2.0.2",
    "gulp": "^3.9.0",
    "gulp-load-plugins": "^1.0.0",
    "jasmine": "^2.3.2",
    "rimraf": "^2.4.3",
    "ts-node": "^0.2.4",
    "tsd": "^0.6.5",
    "tslint": "^2.5.1",
    "typescript": "^1.6.2",
    "watch": "^0.16.0"
  }
}
